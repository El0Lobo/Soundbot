DISCORD_TOKEN=your_bot_token_here
CLIENT_ID=your_application_client_id_here
DEFAULT_BOT_CHANNEL_ID=
IDLE_VC_TIMEOUT_MS=180000
# temp cleanup cadence/age (hours)
TMP_CLEAN_INTERVAL_HOURS=5
TMP_MAX_FILE_AGE_HOURS=5

# Role allowed to upload/import sounds
UPLOAD_ALLOWED_ROLE=Karen-Manager

# Loop channel auto-play (optional)
# When someone joins this voice channel, LOOP_SOUND_ID will loop until empty.
LOOP_CHANNEL_NAME=Narnia
# Sound id from sounds.json to loop
LOOP_SOUND_ID=airhorn
# Volume for loop playback (0.0-1.0)
LOOP_VOLUME=0.5

# Optional: comma list of admin Discord user IDs (for /admin metadata editor)
ADMIN_USER_IDS=

PORT=3000
